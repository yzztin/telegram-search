    "h3": "^1.15.3",
    "listhen": "^1.9.0",
    "tsx": "^4.19.4",
    "unbuild": "^3.5.0",
    "vite-node": "^3.1.3",
    "web-streams-polyfill": "^4.1.0"
  },
  "devDependencies": {
    "@types/node": "^22.15.18",
    "vitest": "^3.1.3"
  }
